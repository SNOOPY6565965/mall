{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getHistoryList, setHistoryList } from '@/utils/storage';\nexport default {\n  name: 'SearchPage',\n  data() {\n    return {\n      search: '',\n      // 输入框的内容\n      // history: ['手机', '小米14', '电视', '牛奶'] // 历史记录\n      history: getHistoryList() // 历史记录\n    };\n  },\n  methods: {\n    goSearch(key) {\n      // indexOf() 方法返回数组中第一次出现给定元素的下标，如果不存在则返回 -1。\n      const index = this.history.indexOf(key);\n      if (index !== -1) {\n        // 存在相同的项，将原有的关键字移除\n        // splice（从哪开始，删除几个，项1，项2）\n        this.history.splice(index, 1);\n      }\n      this.history.unshift(key);\n      setHistoryList(this.history);\n\n      // 跳转到搜索列表页\n      this.$router.push(`/searchList/?search=${key}`);\n    },\n    clear() {\n      this.history = [];\n      setHistoryList([]);\n    }\n  }\n};","map":{"version":3,"names":["getHistoryList","setHistoryList","name","data","search","history","methods","goSearch","key","index","indexOf","splice","unshift","$router","push","clear"],"sources":["src/views/search/index.vue"],"sourcesContent":["<!-- 搜索页 - 一级路由 -->\r\n<template>\r\n  <div>\r\n    <van-nav-bar\r\n      title=\"商品搜索\"\r\n      left-arrow\r\n      @click-left=\"$router.push('/first')\"\r\n    />\r\n    <!-- 搜索 -->\r\n    <div>\r\n      <van-search\r\n        class=\"search\"\r\n        v-model=\"search\"\r\n        placeholder=\"请输入搜索关键词\"\r\n      >\r\n      </van-search>\r\n      <button class=\"btn\" @click=\"goSearch(search)\">搜索</button>\r\n    </div>\r\n    <div v-if=\"history.length > 0\">\r\n      <span>最近搜索</span>\r\n      <van-icon @click=\"clear()\" name=\"delete-o\" class=\"del\"></van-icon>\r\n      <ul v-for=\"item in history\" :key=\"item\">\r\n        <li\r\n        @click=\"goSearch(item)\"\r\n        >\r\n        {{ item }}\r\n      </li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getHistoryList, setHistoryList } from '@/utils/storage'\r\n\r\nexport default {\r\n  name: 'SearchPage',\r\n  data () {\r\n    return {\r\n      search: '', // 输入框的内容\r\n      // history: ['手机', '小米14', '电视', '牛奶'] // 历史记录\r\n      history: getHistoryList() // 历史记录\r\n    }\r\n  },\r\n  methods: {\r\n    goSearch (key) {\r\n      // indexOf() 方法返回数组中第一次出现给定元素的下标，如果不存在则返回 -1。\r\n      const index = this.history.indexOf(key)\r\n      if (index !== -1) {\r\n        // 存在相同的项，将原有的关键字移除\r\n        // splice（从哪开始，删除几个，项1，项2）\r\n        this.history.splice(index, 1)\r\n      }\r\n      this.history.unshift(key)\r\n\r\n      setHistoryList(this.history)\r\n\r\n      // 跳转到搜索列表页\r\n      this.$router.push(`/searchList/?search=${key}`)\r\n    },\r\n    clear () {\r\n      this.history = []\r\n      setHistoryList([])\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import '@/styles/css/search.css';\r\n</style>\r\n"],"mappings":";AAiCA,SAAAA,cAAA,EAAAC,cAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;MAAA;MACA;MACAC,OAAA,EAAAL,cAAA;IACA;EACA;EACAM,OAAA;IACAC,SAAAC,GAAA;MACA;MACA,MAAAC,KAAA,QAAAJ,OAAA,CAAAK,OAAA,CAAAF,GAAA;MACA,IAAAC,KAAA;QACA;QACA;QACA,KAAAJ,OAAA,CAAAM,MAAA,CAAAF,KAAA;MACA;MACA,KAAAJ,OAAA,CAAAO,OAAA,CAAAJ,GAAA;MAEAP,cAAA,MAAAI,OAAA;;MAEA;MACA,KAAAQ,OAAA,CAAAC,IAAA,wBAAAN,GAAA;IACA;IACAO,MAAA;MACA,KAAAV,OAAA;MACAJ,cAAA;IACA;EACA;AAEA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}